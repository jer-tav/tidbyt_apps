load("render.star", "render")
load("http.star", "http")
load("time.star", "time")
load("encoding/base64.star", "base64")

VAR_NIGHTSCOUT_ID="NIGHTSCOUT_ID"
VAR_NIGHTSCOUT_URL="https://" + VAR_NIGHTSCOUT_ID + ".herokuapp.com/api/v1/entries.json"
VAR_NORMAL_RANGE_HIGH=150
VAR_NORMAL_RANGE_LOW=100
VAR_URGENT_RANGE_HIGH=200
VAR_URGENT_RANGE_LOW=70

COLOR_RED = "#f00"
COLOR_YELLOW = "#ff0"
COLOR_GREEN = "#0f0"
COLOR_GREY = "#666"
COLOR_WHITE = "#fff"

def main():
    # Request latest entries from the Nightscout URL
    resp = http.get(VAR_NIGHTSCOUT_URL)

    if resp.status_code != 200:
        fail("Failed to retieve the Nightscout details with status %d", resp.status_code)

    latest_reading = resp.json()[0]
    previous_reading = resp.json()[1]

    # Get date / time strings for latest, previous and now.
    latest_reading_dt = time.parse_time(latest_reading["dateString"])
    previous_reading_dt = time.parse_time(previous_reading["dateString"])
    now_dt = time.now().in_location("UTC")

    # Current sgv value
    sgv_current = latest_reading["sgv"]

    # Used for finding the icon later..
    # Default state is yellow to make the conditions easier
    font_color = COLOR_YELLOW
    color_str = "Yellow"

    if (time.parse_duration("15m") < (now_dt - latest_reading_dt)):
        # The information is stale (i.e. over 5 minutes old) - overrides everything.
        color_str = "Grey"
        font_color = COLOR_GREY
    elif (sgv_current <= VAR_NORMAL_RANGE_HIGH and sgv_current >= VAR_NORMAL_RANGE_LOW):
        # We're in the normal range, so use green.
        font_color = COLOR_GREEN
        color_str = "Green"
    elif (sgv_current >= VAR_URGENT_RANGE_HIGH and sgv_current <= VAR_URGENT_RANGE_LOW):
        # We're in the urgent range, so use red.
        font_color = COLOR_RED
        color_str = "Red"

    # Calculate the delta and format it
    sgv_delta = sgv_current - previous_reading["sgv"]

    str_delta = str(int(sgv_delta))
    if (sgv_delta < 0):
        str_delta = "-" + str_delta
    else:
        str_delta = "+" + str_delta

    # Get the trend - one of DoubleDown, SingleDown, FortyFiveDown, Flat, FortyFiveUp, SingleUp, DoubleUp
    images = {
        "DoubleDown_Green":base64.decode(""),
        "DoubleDown_Grey":base64.decode(""),
        "DoubleDown_Red":base64.decode(""),
        "DoubleDown_Yellow":base64.decode(""),
        "DoubleUp_Green":base64.decode(""),
        "DoubleUp_Grey":base64.decode(""),
        "DoubleUp_Red":base64.decode(""),
        "DoubleUp_Yellow":base64.decode(""),
        "Flat_Green":base64.decode(""),
        "Flat_Grey":base64.decode(""),
        "Flat_Red":base64.decode(""),
        "Flat_Yellow":base64.decode(""),
        "FortyFiveDown_Green":base64.decode(""),
        "FortyFiveDown_Grey":base64.decode(""),
        "FortyFiveDown_Red":base64.decode(""),
        "FortyFiveDown_Yellow":base64.decode(""),
        "FortyFiveUp_Green":base64.decode(""),
        "FortyFiveUp_Grey":base64.decode(""),
        "FortyFiveUp_Red":base64.decode(""),
        "FortyFiveUp_Yellow":base64.decode(""),
        "SingleDown_Green":base64.decode(""),
        "SingleDown_Grey":base64.decode(""),
        "SingleDown_Red":base64.decode(""),
        "SingleDown_Yellow":base64.decode(""),
        "SingleUp_Green":base64.decode(""),
        "SingleUp_Grey":base64.decode(""),
        "SingleUp_Red":base64.decode(""),
        "SingleUp_Yellow":base64.decode("")
    }

    trend = latest_reading["trend"]
    icon_name = trend + "_" + color_str

    return render.Root(
        render.Box(
            render.Row(
                main_align="space_evenly",
                cross_align="center",
                expanded=True,
                children=[
                    render.Text(
                        content=str(int(sgv_current)),
                        font="6x13",
                        color=font_color
                    ),
                    render.Column(
                        main_align="space_evenly",
                        cross_align="center",
                        children=[
                            render.Image(
                                src=images[icon_name]
                            ),
                            render.Text(
                                content=str_delta,
                                color=COLOR_WHITE
                            )    
                        ]
                    )
                ]
            )
        )
    )
